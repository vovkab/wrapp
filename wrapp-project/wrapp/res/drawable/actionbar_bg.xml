<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#A88021" />
        </shape>
    </item>

    <item android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFF95" />
        </shape>
    </item>

    <item
        android:top="1dp"
        android:bottom="1dp">
        <shape>
            <gradient
                android:angle="270"
                android:startColor="#FCE26E"
                android:endColor="#FAC034"
                />
        </shape>
    </item>

</layer-list>
